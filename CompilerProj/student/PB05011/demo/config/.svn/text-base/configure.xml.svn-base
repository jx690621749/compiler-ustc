<?xml version="1.0"?>

<configs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="configure.xsd">
  <boolCfgs>
    <boolCfg name="debug" value="true"/>
    <boolCfg name="dispAST" value="false"/>
    <boolCfg name="doInterp" value="false"/>
    <boolCfg name="doCheck" value="false"/>
    <boolCfg name="doOpt" value="false"/>
    <boolCfg name="dispOptAST" value="false"/>
    <boolCfg name="genAsm" value="true"/>
    <boolCfg name="exec" value="true"/>
  </boolCfgs>
  
  <strCfgs>
    <strCfg name="srcFile" value=""/>
    <strCfg name="asmFile" value="a.s"/>
    <strCfg name="exeFile" value="a.out"/>
    <strCfg name="parserClass" value="edu.ustc.cs.compile.minijool.Parser"/>
    <strCfg name="checkerClass" value="edu.ustc.cs.compile.minijool.Checker"/>
    <strCfg name="interpClass" value="edu.ustc.cs.compile.minijool.Interpreter"/>
    <strCfg name="genClass" value="edu.ustc.cs.compile.minijool.Generator"/>
    <strCfg name="optClass" value="edu.ustc.cs.compile.minijool.Optimizer"/>
    <strCfg name="dumpClass" value="edu.ustc.cs.compile.platform.utils.ASTView.plugin.GenericPropertyDump"/>
    <strCfg name="spimPath" value="/usr/local/bin/spim"/>
    <strCfg name="gccPath" value="/usr/bin/gcc"/>
  </strCfgs>
  
  <archCfgs>
    <arch value="x86"/>
  </archCfgs>
</configs>
